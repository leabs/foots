{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@elements/*": ["src/elements/*"],
      "@features/*": ["src/features/*"],
      "@layouts/*": ["src/layouts/*"]
    },
    "types": ["vitest/globals"],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
